<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.mchack8">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.McHack8">
        <activity
            android:name=".ViewDatabase"
            android:label="@string/title_activity_view_database"
            android:theme="@style/Theme.McHack8.NoActionBar"></activity>
        <activity
            android:name=".DoctorRegisterActivity"
            android:label="@string/title_activity_doctor_register"
            android:theme="@style/Theme.McHack8.NoActionBar" />
        <activity
            android:name=".patientActivity"
            android:label="@string/title_activity_patient"
            android:theme="@style/Theme.McHack8.NoActionBar" />
        <activity
            android:name=".ui.login.LoginPatientActivity"
            android:label="@string/title_activity_login_patient" />
        <activity
            android:name=".DoctorActivity"
            android:label="@string/title_activity_doctor"
            android:theme="@style/Theme.McHack8.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/Theme.McHack8.NoActionBar" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>